    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Main Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/mnt/c/Users/camer/Documents/eecs203/csv2latex/main.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "/mnt/c/Users/camer/Documents/eecs203/csv2latex",
            "environment": [
                {
                    "name": "ASAN_OPTIONS",
                    "value": "abort_on_error=1:detect_leaks=0"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}